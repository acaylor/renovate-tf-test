{
  extends: [
    "config:recommended",
    ":separateMultipleMajorReleases",
    ":ignoreUnstable",
    ":dependencyDashboard",
  ],
  packageRules: [
    {
      matchPackagePatterns: ["renovate-tf-test"],
      matchManagers: ["terraform"],
      enabled: false,
    },
  ],
  minimumReleaseAge: "3 days",
  configMigration: true,
  terraform: {
    pinDigests: true,
    versioning: "regex:^((?<compatibility>.*)-v|v*)(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)$"
  },
}
